    "#all functions\n",
    "\n",
    "def find_album_by_name(name, data):\n",
    "    for hit in data:\n",
    "        if hit['album'] == str(name):\n",
    "            return hit\n",
    "    return None\n",
    "\n",
    "def find_song_by_name(name, data):\n",
    "    for hit in data:\n",
    "        if hit['name'] == str(name):\n",
    "            return hit\n",
    "    return None\n",
    "    \n",
    "def find_by_rank(rank, data):\n",
    "    for hit in data:\n",
    "        if hit['number'] == str(rank):\n",
    "            return hit\n",
    "    return None\n",
    "    \n",
    "def find_by_year(year, data):\n",
    "    for hit in data:\n",
    "        if hit['year'] == str(year):\n",
    "            return hit\n",
    "    return None\n",
    "    \n",
    "def find_by_years(year1, year2, data):\n",
    "    all_hits = []\n",
    "    for hit in data:\n",
    "        if int(hit['year']) >= year1 and int(hit['year']) <= year2:\n",
    "            all_hits.append(hit)  \n",
    "    return all_hits\n",
    "\n",
    "def find_by_ranks(rank1, rank2, data):\n",
    "    all_hits = []\n",
    "    for hit in data:\n",
    "        if int(hit['number']) >= rank1 and int(hit['number']) <= rank2:\n",
    "           all_hits.append(hit)\n",
    "    return all_hits"
