    "import csv\n",
    "\n",
    "with open('data.csv') as f:\n",
    "    reader = csv.DictReader(f)\n",
    "    top_500 = []\n",
    "    titles = []\n",
    "    artists = []\n",
    "    for element in reader:\n",
    "        top_500.append(element)\n",
    "        titles.append(element['album'])\n",
    "        artists.append(element['artist'])\n",
    "print(artists)"
